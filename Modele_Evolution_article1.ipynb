    "def new_resultat(resultat, Mat_network):\n",
    "    l_inf = np.random.choice([-1,1],size=N_inf,p=[1-p,p])\n",
    "    l_suiv=[]\n",
    "    for i in range (N_inf, len(Mat_network)):\n",
    "        for j in Mat_network[i]:\n",
    "            s=0\n",
    "            s+=resultat[j]\n",
    "        if s>0:\n",
    "            l_suiv.append(1)\n",
    "        else:\n",
    "            l_suiv.append(-1)\n",
    "    l_suiv2= np.array(l_suiv)\n",
    "    return np.concatenate((l_inf,l_suiv2))"
   ]
