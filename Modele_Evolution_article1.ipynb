    "def new_network1(Mat_net, Score):\n",
    "    ponder1= np.copy(Score[1])\n",
    "    liste_choix = [s for s in range(N)]\n",
    "    M= np.copy(Mat_net)\n",
    "    for k in range(N_inf,N):\n",
    "        ponder2 = np.copy(Score[1])\n",
    "        l2=[]\n",
    "        for i in M[k]:\n",
    "            l2.append(ponder1[i])\n",
    "            ponder2[i]= 0\n",
    "        i_min = l2.index(min(l2))\n",
    "        j = random.choices(liste_choix, weights= ponder2, k=1)\n",
    "        M[k][i_min]= j[0]\n",
    "    return M"
   ]
