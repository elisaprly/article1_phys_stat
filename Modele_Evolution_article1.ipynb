  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "N= 400\n",
    "z= 0.1 #proportion d'informé\n",
    "N_inf = int(N*z)\n",
    "p= 0.6\n",
    "z2= 0.4# proportion initiale de dandy parmis les suiveurs\n",
    "m= 6\n",
    "g= 0.7 #coeff de memoire\n",
    "a= 0.7 #coeff d'originalité\n",
    "taux= 0.05"
   ]
  },
